=content_for :nav_links do
  =list_of @services do |service|
    =link_to service.name, service

.page-header
  %h1 #{@service.name} #{@time_scale}

#chart
=link_to "< More", service_path(@service, :time_scale => @time_scale, :metric_count => @metric_count.to_i * 2)

:javascript
  $.getJSON('services/#{@service.id}.json?metric_count=#{@metric_count}&time_scale=#{@time_scale}', function(data) {
    $.plot($("#chart"), data[0], data[1] );
  });
