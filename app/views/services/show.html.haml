=content_for :nav_links do
  =list_of @services do |service|
    =link_to service, root_path(:service => service)

.page-header
  %h1= @service

#chart
=link_to "< More", root_path(:service => @service, :time_scale => @time_scale, :metric_count => @metric_count.to_i * 2)

:javascript
  $.getJSON('services/show.json?service=#{@service}&metric_count=#{@metric_count}&time_scale=#{@time_scale}', function(data) {
    $.plot($("#chart"), data[0], data[1] );
  });
